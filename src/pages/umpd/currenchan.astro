---
import UmaLayout from "../../layouts/UmaLayout.astro";
import Music from "../../components/widget/Music.astro";

const title = "Curren Chan";
const cardname = "Fille Éclair";
const date = "11-06-2025";
const rank = "★★★";
const racetype = "turf, sprint";
const strat = "pace, front";
const potential = "3";
const cardimg =
    "https://gametora.com/images/umamusume/characters/chara_stand_1038_103801.png";
---

<img src="/div/underconstruction.webp" />
<UmaLayout
